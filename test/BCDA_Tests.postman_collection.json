									"     pm.expect(pm.response.code).to.be.oneOf([202,200]);",
									"});",
									"",
									"if (pm.response.code === 202) {",
									"    pm.test(\"Has X-Progress header\", function() {",
									"        pm.response.to.have.header(\"X-Progress\", \"In Progress\");",
									"    });",
									"} else if (pm.response.code === 200) {",
									"    const schema = {",
									"        \"properties\": {",
									"            \"transactionTime\": {",
									"                \"type\": \"string\"",
									"            },",
									"            \"request\": {",
									"                \"type\": \"string\"",
									"            },",
									"            \"requiresAccessToken\": {",
									"                \"type\": \"boolean\"",
									"            },",
									"            \"output\": {",
